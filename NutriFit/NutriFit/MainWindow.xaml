<Window x:Class="NutriFit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NutriFit.Views"
        mc:Ignorable="d"
        Title="MainWindow" Width="1920" Height="1080" WindowState="Maximized">
    <Grid>
        <TabControl HorizontalAlignment="Left" Height="1060" Margin="3,4,0,0" VerticalAlignment="Top" Width="1907">
            <TabItem Header="Tab Jela">
                <local:JelaView DataContext="{Binding JelaViewModel}" />
            </TabItem>
            <TabItem Header="Tab Porudžbine">
                <local:PorudzbineView DataContext="{Binding PorudzbineViewModel}" />
            </TabItem>
        </TabControl>

    </Grid>
</Window>
